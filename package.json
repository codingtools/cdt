{
  "name": "@codingtools/cdt",
  "description": "Command Line tools for CODERs",
  "version": "0.1.1",
  "author": "Ashish Patel @ashishpatel0720",
  "bin": {
    "cdt": "./bin/run"
  },
  "bugs": {
    "url": "https://github.com/codingtools/cdt/issues"
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "@types/crypto-js": "^3.1.43",
    "@types/signale": "^1.2.1",
    "crypto-js": "^3.1.9-1",
    "jshashes": "^1.0.7",
    "minify": "^4.1.3",
    "nyc": "^14.1.1",
    "ora": "^4.0.2",
    "signale": "^1.4.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.2",
    "chai": "^4.2.0",
    "globby": "^10.0.1",
    "mocha": "^6.2.2",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/codingtools/cdt",
  "keywords": [
    "cdt",
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cdt",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codingtools/cdt.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  }
}
